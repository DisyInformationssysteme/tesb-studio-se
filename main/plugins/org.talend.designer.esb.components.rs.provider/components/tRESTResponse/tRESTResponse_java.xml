<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT>
	<HEADER AUTHOR="ITOS" RELEASE_DATE="20110805A"
			VERSION="0.101" STATUS="ALPHA"
			COMPATIBILITY="ALL" PLATEFORM="ALL"
			SERIAL="" STARTABLE="false"
			SCHEMA_AUTO_PROPAGATE="false"
			DATA_AUTO_PROPAGATE="false">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>ESB/REST</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<!-- CONNECTOR CTYPE="COMPONENT_ERROR" /> -->
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="RESPONSE_TYPE" FIELD="REST_RESPONSE_SCHEMA_TYPE"
				REQUIRED="true" NUM_ROW="10">
			<ITEMS DEFAULT="DOCUMENT">
				<ITEM NAME="DOCUMENT" VALUE="id_Document" />
				<ITEM NAME="STRING" VALUE="id_String" />
				<ITEM NAME="BYTE_ARRAY" VALUE="id_byte[]" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="STATUS_CODE" FIELD="CLOSED_LIST"
				REQUIRED="true" NUM_ROW="20">
			<ITEMS DEFAULT="200">
				<ITEM NAME="200" VALUE="200" />
				<ITEM NAME="400" VALUE="400" />
				<ITEM NAME="404" VALUE="404" />
				<ITEM NAME="500" VALUE="500" />
<!--
200 OK
300 Multiple Choices
301 Moved Permanently
302 Found
304 Not Modified
307 Temporary Redirect
400 Bad Request
401 Unauthorized
403 Forbidden
404 Not Found
410 Gone
500 Internal Server Error
 -->
				<ITEM NAME="CUSTOM" VALUE="CUSTOM" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="CUSTOM_STATUS_CODE" FIELD="TEXT"
				REQUIRED="true" NUM_ROW="20"
				SHOW_IF="STATUS_CODE == 'CUSTOM'">
			<DEFAULT>200</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE"
				REQUIRED="true" NUM_ROW="28" READONLY="true">
			<TABLE READONLY="true">
				<COLUMN NAME="body" TYPE="id_Document" READONLY="false" NULLABLE="true" />
			</TABLE>
		</PARAMETER>
		
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="RESPONSE_HEADERS" FIELD="TABLE" NB_LINES="4" NUM_ROW="40">
			<ITEMS>
				<ITEM NAME="NAME" VALUE='""'/>
				<ITEM NAME="VALUE" VALUE='""'/>
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="UNWRAP_JSON_RESPONSE" FIELD="CHECK" REQUIRED="true" NUM_ROW="60">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="JSON_ARRAY_KEYS" FIELD="TEXT" REQUIRED="false" NUM_ROW="65" SHOW="true">
          <DEFAULT></DEFAULT>
        </PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
	</CODEGENERATION>

	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer" />
	</RETURNS>

</COMPONENT>
